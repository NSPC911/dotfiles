{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "\u2570\u2500❯ ",
    "foreground": "#D6DEEB",
    "background": "transparent"
  },
  "console_title_template": "@ {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u2007",
          "foreground": "#D6DEEB",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "leading_diamond": "\ue0d7",
          "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }} ",
          "foreground": "#D6DEEB",
          "background": "#575656",
          "type": "os",
          "style": "diamond"
        },
        {
          "template": " \uf09c ",
          "foreground": "#D6DEEB",
          "powerline_symbol": "\ue0b0",
          "background": "#BF616A",
          "type": "root",
          "style": "powerline"
        },
        {
          "template": " \uf489 {{ .Name }} ",
          "foreground": "#011627",
          "powerline_symbol": "\ue0b0",
          "background": "#00C7FC",
          "type": "shell",
          "style": "powerline"
        },
        {
          "options": {
            "display_mode": "context"
          },
          "template": " \ue73c {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Major }}.{{ .Minor }} ",
          "foreground": "#2e3440",
          "powerline_symbol": "\ue0b0",
          "background": "#EBCB8B",
          "type": "python",
          "style": "powerline"
        },
        {
          "template": " \ue865 {{ if .Error }}{{ else }}{{ .Major }}.{{ .Minor }} {{ end }}",
          "foreground": "#2e3440",
          "powerline_symbol": "\ue0b0",
          "background": "#D08770",
          "type": "pnpm",
          "style": "powerline"
        },
        {
          "template": " \ue627 {{ if .Error }}{{ else }}{{ .Major }}.{{ .Minor }} {{ end }}",
          "foreground": "#2e3440",
          "powerline_symbol": "\ue0b0",
          "background": "#88C0D0",
          "type": "go",
          "style": "powerline"
        },
        {
          "type": "lua",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#2e3440",
          "background": "#4CA1D0",
          "template": "  {{ if .Error }}{{ else }}{{ .Major }}.{{ .Minor }} {{ end }}"
        },
        {
          "template": " \ue76f {{ if .Error }}{{ else }}{{ .Major }}.{{ .Minor }} {{ end }}",
          "foreground": "#2e3440",
          "powerline_symbol": "\ue0b0",
          "background": "#5e81ac",
          "type": "bun",
          "style": "powerline"
        },
        {
          "template": " \ue7a8 {{ if .Error }}{{ else }}{{ .Major }}.{{ .Minor }} {{ end }}",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#70899D",
          "type": "rust",
          "style": "powerline"
        },
        {
          "options": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} \ue0b1{{ end }}{{ if .Staging.Changed }}<#CAEBE1> \uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#2e3440",
          "powerline_symbol": "\ue0b0",
          "background": "#88c0d0",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#8FBCBB{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#bf616a{{ end }}",
            "{{ if gt .Ahead 0 }}#B48EAD{{ end }}",
            "{{ if gt .Behind 0 }}#B48EAD{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u256d\u2500",
          "foreground": "#D6DEEB",
          "type": "text",
          "style": "plain"
        },
        {
          "options": {
            "folder_icon": " \uf07c ",
            "folder_separator_icon": " \uf061 ",
            "home_icon": "\uf015",
            "max_depth": 5,
            "style": "unique",
            "mapped_locations": {
              "~/Git": "\uf015 \uf061 \ue5fb",
              "~/.local/share/chezmoi": "\uebd3",
              "~/AppData": "\uf015 \uf061 \udb81\ude75",
              "~/scoop": "\uf015 \uf061 \uef88"
            }
          },
          "template": " {{ .Path }} ",
          "foreground": "#D6DEEB",
          "type": "path",
          "style": "plain"
        },
        {
          "options": {
            "always_enabled": true
          },
          "template": "<#D6DEEB>|</> {{ if gt .Code 0 }}\uf00d {{ .Code }}{{ else }}\uf42e{{ end }} ",
          "foreground": "#03DED3",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E5727A{{ else }}#B1C89D{{ end }}"
          ]
        },
        {
          "options": {
            "always_enabled": true,
            "style": "roundrock",
            "threshold": 0
          },
          "template": "in {{ .FormattedMs }}",
          "foreground": "#D6DEEB",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#D6DEEB",
          "template": "{{ if .Env.WEZTERM_PANE }} |  {{ .Env.WEZTERM_PANE }} {{ end }}"
        },
        {
          "type": "peard",
          "style": "plain",
          "foreground": "#D6DEEB",
          "options": {
            "port": 26538,
            "playing_icon": " ",
            "paused_icon": " "
          },
          "template": "| {{ .Icon }}{{ if ne .Status \"paused\" }}{{ .Artist }} - {{ .Track }}{{ end }}"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u2570\u2500",
          "foreground": "#D6DEEB",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "❯ ",
          "foreground": "#D6DEEB",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "shell_integration": true,
  "patch_pwsh_bleed": true
}
